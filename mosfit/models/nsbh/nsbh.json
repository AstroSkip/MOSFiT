{
    "@references":[
        {
            "name":"Gompertz et al., in preparation"
        }
    ],
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "rvhost":{
        "kind":"parameter",
        "value":3.1,
        "class":"parameter",
        "latex":"R_{\\rm V, host}"
    },
    "frad":{
        "kind":"parameter",
        "value":0.999,
        "class":"parameter",
        "latex":"f_{\\rm rad}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e16,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":-0.47406,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature":{
        "kind":"parameter",
        "value":2500,
        "class":"parameter",
        "latex":"T_{\\rm rec}\\,{\\rm (K)}"
    },
    "kappa_red":{
        "kind":"parameter",
        "value":5.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappa_purple":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappa_black":{
        "kind":"parameter",
        "value":15.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10000.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "Mchirp":{
        "kind":"parameter",
        "value":2.42,
        "class":"parameter",
        "latex":"M_{\\rm chirp}\\,(M_\\odot)",
        "derived_keys":[
            "M1",
            "M2",
            "mejecta_magnetic",
            "mejecta_thermal",
            "mejecta_dyn",
            "vejecta_magnetic",
            "vejecta_thermal",
            "vejecta_dyn",
            "Mrem",
            "Mdisc",
            "f_ej"
        ]
    },
    "q":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"q=M_{2}/M_{1}"
    },
    "chi":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"\\chi_{\\rm BH}"
    },
    "cos_theta":{
        "kind":"parameter",
        "value":0.9,
        "class":"parameter",
        "latex":"\\cos\\theta"
    },
    "cos_theta_open":{
        "kind":"parameter",
        "value":0.866,
        "class":"parameter",
        "latex":"\\cos\\theta_{\\rm open}",
        "derived_keys":[
            "area_blue",
            "area_blue_ref",
            "area_red",
            "area_red_ref",
            "area_dyn",
            "area_dyn_ref"
        ]
    },
    "cos_theta_open_dyn":{
        "kind":"parameter",
        "value":0.1736,
        "class":"parameter",
        "latex":"\\cos\\theta_{\\rm open\\, dyn}"
    },
    "radius_ns":{
        "kind":"parameter",
        "value":12.0,
        "class":"parameter",
        "latex":"R_{\\rm NS}\\,({\\rm km})"
    },
    "Mtov":{
        "kind":"parameter",
        "value":2.1,
        "class":"parameter",
        "latex":"M_{\\rm TOV}\\,(M_\\odot)"
    },
    "s":{
        "kind":"parameter",
        "value":1,
        "class":"parameter",
        "latex":"s"
    },
    "errMdyn":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"errMdyn"
    },
    "errMdisk":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"errMdisk"
    },
    "fMag":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"f_{\\rm mag}"
    },
    "nsbh_ejecta":{
        "kind":"energetic",
        "inputs":[
            "Mchirp",
            "q",
            "chi",
            "Mtov",
            "radius_ns",
            "errMdyn",
            "errMdisk",
            "fMag"
        ]
    },
    "aspherical_kilonova":{
        "kind":"energetic",
        "inputs":[
            "cos_theta",
            "cos_theta_open"
        ]
    },
    "aspherical_wind":{
        "kind":"energetic",
        "inputs":[
            "aspherical_kilonova",
            "cos_theta",
            "cos_theta_open_dyn"
        ]
    },
    "rprocess":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "frad",
            "texplosion",
            "nsbh_ejecta",
            "kappa_purple"
        ],
        "replacements":{
            "mejecta":"mejecta_thermal",
            "luminosities":"luminosities_thermal",
            "vejecta":"vejecta_thermal",
            "kappa":"kappa_purple"
        }
    },
    "diffusion_aspherical":{
        "kind":"transform",
        "inputs":[
            "nsbh_ejecta",
            "aspherical_wind",
            "rprocess",
            "kappa_purple",
            "kappagamma",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"luminosities_thermal",
            "mejecta":"mejecta_thermal",
            "vejecta":"vejecta_thermal",
            "kappa":"kappa_purple",
            "area_proj":"area_red",
            "area_ref":"area_red_ref"
        }
    },
    "temperature_floor":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "temperature",
            "diffusion_aspherical"
        ],
        "replacements":{
            "luminosities": "luminosities_thermal",
            "vejecta":"vejecta_thermal",
            "mejecta":"mejecta_thermal",
            "kappa":"kappa_purple"
        }
    },
    "blackbody":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_thermal"
        }
    },
    "rprocess2":{
        "kind":"engine",
        "class":"rprocess",
        "inputs":[
            "densetimes",
            "frad",
            "texplosion",
            "nsbh_ejecta",
            "kappa_red"
        ],
        "replacements":{
            "mejecta":"mejecta_magnetic",
            "luminosities":"luminosities_magnetic",
            "vejecta":"vejecta_magnetic",
            "kappa":"kappa_red"
        }
    },
    "diffusion2":{
        "kind":"transform",
        "class":"diffusion_aspherical",
        "inputs":[
            "rprocess2",
            "aspherical_wind",
            "kappa_red",
            "kappagamma",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"luminosities_magnetic",
            "mejecta":"mejecta_magnetic",
            "vejecta":"vejecta_magnetic",
            "kappa":"kappa_red",
            "area_proj":"area_blue",
            "area_ref":"area_blue_ref"
        }
    },
    "temperature_floor2":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "temperature",
            "diffusion2"
        ],
        "replacements":{
            "luminosities": "luminosities_magnetic",
            "vejecta":"vejecta_magnetic",
            "mejecta":"mejecta_magnetic",
            "kappa":"kappa_red"
        }
    },
    "blackbody2":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor2",
            "blackbody"
        ],
        "requests":{
            "band_wave_ranges": "photometry",
            "seds":"blackbody"
        },
        "replacements":{
            "luminosities":"luminosities_magnetic"
        }
    },
    "rprocess3":{
        "kind":"engine",
        "class":"rprocess",
        "inputs":[
            "densetimes",
            "frad",
            "texplosion",
            "nsbh_ejecta",
            "kappa_black"
        ],
        "replacements":{
            "mejecta":"mejecta_dyn",
            "luminosities":"luminosities_dyn",
            "vejecta":"vejecta_dyn",
            "kappa":"kappa_black"
        }
    },
    "diffusion3":{
        "kind":"transform",
        "class":"diffusion_aspherical",
        "inputs":[
            "rprocess3",
            "aspherical_wind",
            "kappa_black",
            "kappagamma",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"luminosities_dyn",
            "mejecta":"mejecta_dyn",
            "vejecta":"vejecta_dyn",
            "kappa":"kappa_black",
            "area_proj":"area_dyn",
            "area_ref":"area_dyn_ref"
        }
    },
    "temperature_floor3":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "temperature",
            "diffusion3"
        ],
        "replacements":{
            "luminosities":"luminosities_dyn",
            "vejecta":"vejecta_dyn",
            "mejecta":"mejecta_dyn",
            "kappa":"kappa_black"
        }
    },
    "blackbody3":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor3",
            "blackbody2"
        ],
        "requests":{
            "band_wave_ranges": "photometry",
            "seds":"blackbody2"
        },
        "replacements":{
            "luminosities":"luminosities_dyn"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody3",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_dyn"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs":[
            "losextinction",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"luminosities_dyn"
        }
    },
    "nsbh_constraints":{
        "kind":"constraint",
        "inputs":[
            "Mtov",
            "radius_ns",
            "nsbh_ejecta"
        ]
    },
    "likelihood":{
        "kind":"objective",
        "supports":[
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs":[
            "diagonal",
            "nsbh_constraints"
        ]
    }
}