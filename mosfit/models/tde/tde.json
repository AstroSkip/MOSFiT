{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}"
    },
    "b":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"b\\ (scaled\\ \\beta)"
    },
    "efficiency":{
        "kind":"parameter",
        "value":0.05,
        "class":"parameter",
        "latex":"\\epsilon\\ (efficiency)"
    },
    "tpeak":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"t_{\\rm peak}"
    },
    "Tviscous":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"\\tau_{\\rm viscous}/t_{\\rm peak}"
    },
    "bhmass":{
        "kind":"parameter",
        "value":1.0e6,
        "class":"parameter",
        "latex":"M_{h} (M_{odot})"
    },
    "starmass":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{star} (M_{odot})"
    },
    "Rstar":{
        "kind":"parameter",
        "value": 1.0,
        "class": "parameter",
        "latex":"R_{star} (R_{odot})"
    },
    "Rph0":{
        "kind":"parameter",
        "value": 0.17,
        "class":"parameter",
        "latex": "log(R_{ph0})"
    },
    "lphoto":{
        "kind": "parameter",
        "value": 2.7,
        "class": "parameter",
        "latex": "photosphere exponent 'l' "
    },
    "lumdist":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter"
    },
    "fallback":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "bhmass",
            "starmass",
            "b",
            "efficiency",
            "tpeak",
            "texplosion"
        ]
    },
    "viscous":{
        "kind":"transform",
        "inputs":[
            "fallback",
            "kappa",
            "texplosion",
            "Tviscous"
        ]
    },
    "tde_photosphere":{
        "kind":"photosphere",
        "inputs":[
            "bhmass",
            "starmass",
            "lphoto",
            "Rph0",
			"viscous"
        ]
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "tde_photosphere"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        }
    },
    "photometry":{
        "kind":"observable",
        "inputs":[
            "losextinction",
            "lumdist"
        ]
    }
}
