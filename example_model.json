{
    "transient":{
        "kind":"data",
        "keys":{
            "photometry":[
                "time",
                "magnitude",
                "e_magnitude",
                "band"
            ],
            "lumdist":[
                "value"
            ],
            "redshift":[
                "value"
            ]
        }
    },
    "mnickel":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter"
    },
    "temperature":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter"
    },
    "mejecta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter"
    },
    "vejecta":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter"
    },
    "variance":{
        "kind":"parameter",
        "class":"parameter"
    },
    "nickelcobalt":{
        "kind":"engine",
        "inputs":[
            "transient",
            "mnickel",
            "texplosion"
        ],
        "outputs":"luminosities"
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "nickelcobalt",
            "kappa",
            "kappagamma",
            "mejecta",
            "texplosion",
            "vejecta"
        ],
        "outputs":"luminosities"
    },
    "photosphere":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "diffusion",
            "temperature"
        ],
        "outputs":"seds"
    },
    "filter":{
        "bands":["U", "B", "V", "R", "I"],
        "kind":"observable",
        "paths":[
            "bess_u.dat",
            "bess_b.dat",
            "bess_v.dat",
            "bess_r.dat",
            "bess_i.dat"
        ],
        "inputs":[
            "photosphere",
            "transient"
        ],
        "requests":[
            ["bandwavelengths", "bandnames"],
            []
        ],
        "outputs":"model_magnitudes"
    },
    "likelihood":{
        "kind":"objective",
        "inputs":[
            "transient",
            "variance",
            "filter"
        ]
    },
    "fitlc":{
        "kind":"output",
        "inputs":[
            "likelihood",
            "transient",
            "filter"
        ],
        "class":"lightcurve"
    }
}
