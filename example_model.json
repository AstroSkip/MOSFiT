{
    "transient":{
        "kind":"data",
        "keys":{
            "photometry":[
                "time",
                "magnitude",
                "e_magnitude",
                "band"
            ],
            "lumdist":[
                "value"
            ]
        }
    },
    "mnickel":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter"
    },
    "temperature":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter"
    },
    "variance":{
        "kind":"parameter",
        "class":"parameter"
    },
    "nickelcobalt":{
        "kind":"engine",
        "inputs":[
            "transient",
            "mnickel",
            "texplosion"
        ],
        "outputs":"luminosities"
    },
    "identity":{
        "kind":"transform",
        "inputs":"nickelcobalt",
        "outputs":"luminosities"
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "identity",
            "temperature"
        ],
        "outputs":"seds"
    },
    "filter":{
        "bands":["B", "V", "R", "I"],
        "kind":"observable",
        "paths":[
            "observables/filters/bess_b.dat",
            "observables/filters/bess_v.dat",
            "observables/filters/bess_r.dat",
            "observables/filters/bess_i.dat"
        ],
        "inputs":[
            "blackbody",
            "transient"
        ],
        "requests":[
            ["wavelengths", "bands"],
            []
        ],
        "outputs":"model_magnitudes"
    },
    "likelihood":{
        "kind":"objective",
        "inputs":[
            "transient",
            "variance",
            "filter"
        ]
    }
}
